{
  "name": "page",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "move-html": "node src/scripts/move-html-to-public.js",
    "public-html": "find . -name 'dist/*.html' ! -name 'index.html' -exec cp {} dist/public/ \\;",
    "public-src":"mkdir -p public/src && cp -r src/* public/src/",
    "build": "npm run public-src && && vite build && npm run public-html",
    "preview": "vite preview",
    "lint:fix": "eslint . --fix",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/vite": "^4.1.4",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "vite": "^6.3.1"
  }
}
